<Window x:Class="SearchBoxDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchBoxDemo"
        xmlns:lib="clr-namespace:TMTech.Shared.WPFLIB.Controls;assembly=TMTech.Shared.WPFLIB"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <lib:SearchTextBox DockPanel.Dock="Top" Margin="5"></lib:SearchTextBox>
        <TextBox DockPanel.Dock="Top" Margin="5">dsf</TextBox>
        <TreeView BorderThickness="0 1 0 0">
            <TreeViewItem Header="Apple"></TreeViewItem>
            <TreeViewItem Header="Pear"></TreeViewItem>
            <TreeViewItem Header="Orrange"></TreeViewItem>
            <TreeViewItem Header="Peach"></TreeViewItem>
        </TreeView>
    </DockPanel>
</Window>
